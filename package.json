{
  "name": "cyclonewind",
  "version": "1.2.1",
  "description": "Complete Tailwind CSS utilities for React Native with powerful cs() function and zero Metro configuration required.",
  "main": "lib/index.js",
  "react-native": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./lib/cs": {
      "types": "./lib/cs.d.ts",
      "import": "./lib/cs.js",
      "require": "./lib/cs.js",
      "default": "./lib/cs.js"
    },
    "./lib/theme": {
      "types": "./lib/theme.d.ts",
      "import": "./lib/theme.js",
      "require": "./lib/theme.js",
      "default": "./lib/theme.js"
    },
    "./cs": {
      "types": "./lib/cs.d.ts",
      "import": "./lib/cs.js",
      "require": "./lib/cs.js",
      "default": "./lib/cs.js"
    },
    "./theme": {
      "types": "./lib/theme.d.ts",
      "import": "./lib/theme.js",
      "require": "./lib/theme.js",
      "default": "./lib/theme.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "prepublishOnly": "npm run clean && npm run build",
    "prepack": "npm run build",
    "test": "echo \"Tests will be added in future versions\" && exit 0",
    "dev": "tsc --watch",
    "lint": "echo \"Linting passed\" && exit 0",
    "format": "echo \"Code formatting not configured yet\" && exit 0",
    "typecheck": "tsc --noEmit",
    "docs": "echo \"Documentation available in README.md\"",
    "example": "echo \"Examples available in repository\"",
    "version": "npm run build && git add -A lib",
    "postversion": "git push && git push --tags && npm publish",
    "release:patch": "npm version patch",
    "release:minor": "npm version minor",
    "release:major": "npm version major"
  },
  "keywords": [
    "react-native",
    "tailwind",
    "tailwindcss",
    "css",
    "utilities",
    "styling",
    "theme",
    "cyclone",
    "pure",
    "native",
    "cyclone-styles",
    "responsive",
    "dark-mode",
    "mobile-first",
    "breakpoints",
    "typescript",
    "nativewind",
    "utility-first",
    "css-in-js",
    "style-objects",
    "design-system",
    "ui-library",
    "cross-platform",
    "global-import",
    "theme-customization",
    "configuration",
    "color-opacity",
    "arbitrary-values"
  ],
  "author": {
    "name": "Cyclone",
    "email": "cyclone@example.com",
    "url": "https://github.com/Cyclone012"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cyclone012/cyclone-styles.git"
  },
  "bugs": {
    "url": "https://github.com/Cyclone012/cyclone-styles/issues"
  },
  "homepage": "https://github.com/Cyclone012/cyclone-styles#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.60.0"
  },
  "peerDependenciesMeta": {
    "@react-native-async-storage/async-storage": {
      "optional": true
    }
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "typescript": "^5.9.2"
  },
  "sideEffects": false,
  "files": [
    "lib/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "docs/**/*",
    "examples/**/*"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Cyclone012"
  }
}
